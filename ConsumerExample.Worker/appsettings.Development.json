{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    },
    "QueuConfiguration": {
      "QueueBloqueio": {
        "Journey": "Bloqueio",
        "QueueUrl": "",
        "MaxNumberOfMessages": 10,
        "WaitTimeSeconds": 20
      },
      "QueueDesbloqueio": {
        "Journey": "Desbloqueio",
        "QueueUrl": "",
        "MaxNumberOfMessages": 10,
        "WaitTimeSeconds": 20
      }
    }
  }
}
